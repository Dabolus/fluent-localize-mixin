{
  "extends": "eslint-config-google",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "impliedStrict": true
    },
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "max-len": [
      "error",
      120
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "curly": [
      "error",
      "all"
    ],
    "object-curly-spacing": ["error", "always"],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "no-undef": "error"
  }
}
